
  <ul class="pagination">
    {% if paginate.previous.is_link %}
    <li>
      
      <a href="{{ paginate.previous.url }}" class="des-font border"><i class="ti-arrow-left"></i></a>
      
    </li>
    {% endif %}
    {% assign count = paginate.pages %}
    {% for part in (1..count) %}
    <li><a class="{% if paginate.current_page == part %}active{% endif %} des-font" href="{{ collection.url }}?page={{ forloop.index }}">{{ forloop.index }}</a></li>
    {% endfor %}
    {% if paginate.next.is_link %}
    <li>
      
      <a href="{{ paginate.next.url }}" class="des-font border"><i class="ti-arrow-right"></i></a>
      
    </li>
    {% endif %}
  </ul>
