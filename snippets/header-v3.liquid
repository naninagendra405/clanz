<header class="BG-header-1 full-width fixed header-home2 header_v3">
  <div class="container space_top_bot_30 delay03" id="menu-header">
    <div class="row flex">
      <div class="col-lg-3 col-md-3 col-sm-6 col-xs-5 logo-top-home1">
        <a href="/"><img src="{% if settings.logo-image-hdv3 == blank %}{{'logo.png' | asset_url}}{% else %}{{ settings.logo-image-hdv3 | img_url:'master' }}{% endif %}" alt="logo" class="img-logo"></a>
      </div>
      
      <div class="hidden_992 col-lg-6 col-md-6 hidden-sm hidden-xs menu-center-home1 menu_sticky_v3 flex center">
        {% include 'horizon-menu-nixx-2' %}
      </div>
      
      <div class="hidden_992 col-lg-3 col-md-3 col-sm-6 col-xs-7 text-right icon-main icon_sticky_v3">
        <a href="#" class="link-menu delay03 container_20 inline-block hidden-xs hidden-sm" id="btn-search">
          {% if settings.icon-search == blank %}
          <i class="ti-search"></i>
          {% else %}
          <i class="{{ settings.icon-search }}"></i>
          {% endif %}
        </a>
        {% if customer.email != blank %}
         <a class="link-menu delay03 container_20 inline-block hidden-xs hidden-sm" href="/account/logout" title="logout">
           {% if settings.icon-logout == blank %}
           <i class="zoa-icon-log-out"></i>
           {% else %}
          <i class="{{ settings.icon-logout }}"></i>
          {% endif %}
        </a>
            {% else %}
        <a href="/account/login" class="link-menu delay03 container_20 inline-block hidden-xs hidden-sm" id="btn-login">
          {% if settings.icon-login == blank %}
          <i class="zoa-icon-user"></i>
          {% else %}
          <i class="{{ settings.icon-login }}"></i>
          {% endif %}
        </a>
        {% endif %}
        <a href="#" class="link-menu delay03 container_20 relative inline-block text-center" id="btn-cart">
          {% if settings.icon-cart == blank %}
          <i class="ti-bag"></i>
          {% else %}
          <i class="{{ settings.icon-cart }}"></i>
          {% endif %}
          <figure class="absolute label-cart number-font enj-cartcount">{{ cart.item_count }}</figure>
        </a>
        <a href="#" class="link-menu delay03 inline-block hidden-md hidden-lg space_left_10 btn-push-menu">
          {% if settings.icon-pushmenu == blank %}
          <svg width="26" height="16" version="1.1" id="Layer_1" xmlns="http://www.w3.org/2000/svg" x="0px" y="0px" viewBox="0 0 66 41" xmlns:xlink="http://www.w3.org/1999/xlink" xml:space="preserve">
            <g>
              <line class="st0" x1="1.5" y1="1.5" x2="64.5" y2="1.5"></line>
              <line class="st0" x1="1.5" y1="20.5" x2="64.5" y2="20.5"></line>
              <line class="st0" x1="1.5" y1="39.5" x2="64.5" y2="39.5"></line>
            </g>
          </svg>
          {% else %}
          <i class="{{ settings.icon-pushmenu }}"></i>
          {% endif %}
        </a>
      </div>
    </div>
  </div>
</header>
<div class="absolute hidden-sm hidden-xs menu_home_v3">
  {% include 'horizon-menu-nixx-2' %}
</div>
<div class="absolute hidden-sm hidden-xs icon_home_v3 flex">
  <a href="#" class="link-menu delay03 container_20 relative inline-block text-center btn-cart">
    {% if settings.icon-cart == blank %}
    <i class="ti-bag"></i>
    {% else %}
    <i class="{{ settings.icon-cart }}"></i>
    {% endif %}
    <figure class="absolute label-cart number-font enj-cartcount">{{ cart.item_count }}</figure>
  </a>
  {% if customer.email != blank %}
  <a class="link-menu delay03 container_20 inline-block hidden-xs hidden-sm" href="/account/logout" title="logout">
    {% if settings.icon-logout == blank %}
    <i class="zoa-icon-log-out"></i>
    {% else %}
    <i class="{{ settings.icon-logout }}"></i>
    {% endif %}
  </a>
  {% else %}
  <a href="/account/login" class="link-menu delay03 container_20 inline-block hidden-xs hidden-sm">
    {% if settings.icon-login == blank %}
    <i class="zoa-icon-user"></i>
    {% else %}
    <i class="{{ settings.icon-login }}"></i>
    {% endif %}
  </a>
  {% endif %}
  <a href="#" class="link-menu delay03 container_20 inline-block hidden-xs hidden-sm btn-search">
    {% if settings.icon-search == blank %}
    <i class="ti-search"></i>
    {% else %}
    <i class="{{ settings.icon-search }}"></i>
    {% endif %}
  </a>
</div>