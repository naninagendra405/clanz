  <div class="row flex collection_content_1">
    <div class="content clear-space relative">
      <a href="{{section.settings.url_1}}" class="over-hidden inline-block">
        {% if section.settings.img_1 == blank %}
        <img src="{{'860x960.jpg' | asset_url}}" class="img-responsive hover-zoom-out" alt="">
        {% else %}
        <img src="{{section.settings.img_1 | img_url:'master'}}" class="img-responsive hover-zoom-out" alt="">
        {% endif %}
      </a>
      {% if section.settings.btn_collection_1 %}
      <div class="button_collection absolute">
        <a href="{{section.settings.url_1}}" class="inline-block title-font">{{section.settings.btn_name_1}}</a>
      </div>
      {% endif %}
      {% if section.settings.enable_btn_shop_1 %}
      <div class="button_shop absolute">
        <a href="{{section.settings.url_1}}" class="inline-block menu-font">{{section.settings.btn_shop_1}}</a>
      </div>
      {% endif %}
