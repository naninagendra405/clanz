<div data-section-id="{{ section.id }}" style="{% if section.settings.margin_top != blank %}margin-top: {{section.settings.margin_top}}px;{%endif%} {% if section.settings.margin_bottom != blank %}margin-bottom: {{section.settings.margin_bottom}}px;{% endif %}">

<div class="insta_home3 {% if section.settings.box == 'box' %}container{% else %}container-fluid{% endif %} relative">
  <div {% if section.settings.box == 'full' %}class="row"{% endif %}>
	{% if section.blocks.size > 0 %}
  	{% for block in section.blocks %}
  {% if block.type == '5_image' %}
  <div class="column-20 relative">
    <a href="{{block.settings.url}}" class="inline-block over-hidden">
      {% if block.settings.img == blank %}
      <img src="{{'img_insta_home3_1.jpg' | asset_url}}" class="img-responsive full-width hover-zoom-out" alt="">
      {% else %}
      <img src="{{ block.settings.img | img_url:'master' }}" class="img-responsive full-width hover-zoom-out" alt="">
      {% endif %}
    </a>
    <div class="absolute icon-insta">
      <a href="{{block.settings.url}}" class="text-center des-font absolute inline-block">
        {% if block.settings.icon == blank %}
        <i class="ti-instagram"></i>
        {% else %}
        <i class="{{block.settings.icon}}"></i>
        {% endif %}
      </a>
    </div>
  </div>
    {% elsif block.type == '6_image' %}
    <div class="col-lg-2 col-md-3 col-sm-4 col-xs-6 clear-space relative">
    <a href="{{block.settings.url}}">
      {% if block.settings.img == blank %}
      <img src="{{'img_insta_home3_1.jpg' | asset_url}}" class="img-responsive full-width" alt="">
      {% else %}
      <img src="{{ block.settings.img | img_url:'master' }}" class="img-responsive full-width" alt="">
      {% endif %}
    </a>
    <div class="absolute icon-insta text-center">
      <a href="{{block.settings.url}}">
        {% if block.settings.icon == blank %}
        <i class="ti-instagram"></i>
        {% else %}
        <i class="{{block.settings.icon}}"></i>
        {% endif %}
      </a>
    </div>
  </div>
    {% elsif block.type == '2_image' %}
    <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6 relative">
    <a href="{{block.settings.url}}">
      {% if block.settings.img == blank %}
      <img src="{{'img_insta_home3_1.jpg' | asset_url}}" class="img-responsive full-width" alt="">
      {% else %}
      <img src="{{ block.settings.img | img_url:'master' }}" class="img-responsive full-width" alt="">
      {% endif %}
    </a>
    <div class="absolute icon-insta text-center">
      <a href="{{block.settings.url}}">
        {% if block.settings.icon == blank %}
        <i class="ti-instagram"></i>
        {% else %}
        <i class="{{block.settings.icon}}"></i>
        {% endif %}
      </a>
    </div>
  </div>
    {% elsif block.type == '4_image' %}
    <div class="col-lg-3 col-md-3 col-sm-6 col-xs-6 relative">
    <a href="{{block.settings.url}}">
      {% if block.settings.img == blank %}
      <img src="{{'img_insta_home3_1.jpg' | asset_url}}" class="img-responsive full-width" alt="">
      {% else %}
      <img src="{{ block.settings.img | img_url:'master' }}" class="img-responsive full-width" alt="">
      {% endif %}
    </a>
    <div class="absolute icon-insta text-center">
      <a href="{{block.settings.url}}">
        {% if block.settings.icon == blank %}
        <i class="ti-instagram"></i>
        {% else %}
        <i class="{{block.settings.icon}}"></i>
        {% endif %}
      </a>
    </div>
  </div>
  {% endif %}
  {% endfor %}
  {% endif %}
    {% if section.settings.enable_button %}
	<div class="absolute content_button">
      <a href="{{section.settings.url}}">{{section.settings.btn_name}}</a>
    </div>
    {% endif %}
  </div>
</div>
</div>
